  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ua-cron/core.git"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/ua-cron/core/issues"
  },
  "scripts": {
    "build": "npm run clean && tsc && node prepare-package.mjs",
    "clean": "rm -rf dist",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "e2e": "npm run build && npm run e2e-inject && npx cypress open --component",
    "e2e-inject": "npm install ./dist --force",
    "e2e-run": "npm run build && npm run e2e-inject && npx cypress run --component"
  },
  "dependencies": {
    "@sbzen/cron-core": "2.0.0-alpha.1660480017312"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cypress/react": "^6.1.0",
    "@sbzen/e2e": "https://github.com/ua-cron/e2e/releases/download/20220815171625/dist.tar.gz",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^8.2.5",
    "bootstrap": "^4.6.1",
    "eslint": "^8.18.0",
    "jest": "^28.1.0",
    "lodash": "^4.17.21",
    "react-scripts": "^5.0.1",
    "sass": "^1.53.0",
    "storybook-addon-sass-postcss": "^0.1.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "4.6.4"
  }
